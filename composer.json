{
  "name": "admini/removable_control",
  "description": "remote control heat bases",
  "type": "project",
  "authors": [
    {
      "name": "Tirasteplo",
      "email": "tirasteplo@gmail.com"
    }
  ],
  "require": {
    "firebase/php-jwt": "^6.0",
    "paragonie/sodium_compat": "^1.20",
    "fxp/composer-asset-plugin": "*",
    "aldas/modbus-tcp-client": "^0.2.1",
    "adduc/phpmodbus": "^0.8.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "modbus-asset-library": "./vendor/aldas/modbus-tcp-client",
      "phpjwt-asset-library": "./vendor/firebase/php-jwt"
    }
  },
  "config": {
    "vendor-dir": "./vendor"
  }
}